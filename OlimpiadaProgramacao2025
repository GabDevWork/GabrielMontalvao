namespace OlimpiadaProgramacao2025
{
    internal class Program
    {
        static void Main(string[] args)
        {//FINAL
         ///////////////////////////BEECROWD//////////////////////////////////////
            ///cod
            string input1 = Console.ReadLine();
            string input2 = Console.ReadLine();
            string result1 = "", result2 = "";
            int test = 0;
            for(int i = 0; i < input1.Length && result1.Length < 11; i++)
            {
                if (input1[i] == '.')
                {
                    result1 += input1[i];
                }
                else if (input1[i] == '1' ||
                    input1[i] == '2' ||
                    input1[i] == '3' ||
                    input1[i] == '4' ||
                    input1[i] == '5' ||
                    input1[i] == '6' ||
                    input1[i] == '7' ||
                    input1[i] == '8' ||
                    input1[i] == '9' ||
                    input1[i] == '0'
                    )
                {
                    result1 += input1[i];
                }
            }
            for (int i = 0; i < input2.Length && result2.Length < 11; i++)
            {
                if (input2[i] == '.')
                {
                    result1 += input2[i];
                }
                else if (input2[i] == '1' ||
                    input2[i] == '2' ||
                    input2[i] == '3' ||
                    input2[i] == '4' ||
                    input2[i] == '5' ||
                    input2[i] == '6' ||
                    input2[i] == '7' ||
                    input2[i] == '8' ||
                    input2[i] == '9'
                    )
                {
                    result2 += input2[i];
                }
            }

            //double money = double.Parse(input1) + double.Parse(input2);
            Console.WriteLine($"cpf {result1}\n{result2}");
        }
    }
}




