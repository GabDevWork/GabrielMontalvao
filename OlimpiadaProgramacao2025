//EXERCICIO STRING FAZER MAIS TARDE
namespace OlimpiadaProgramacao2025
{
    internal class Program
    {
        static void Main(string[] args)
        {//Treino 23/04/2025
            ///////////////////////////BEECROWD//////////////////////////////////////
            //cod 1248 Plano de Dieta
            int N = int.Parse(Console.ReadLine());
            for (int i = 0; i < N; i++) { 
                string dieta = Console.ReadLine();
                string cafe = Console.ReadLine();
                string jantar = Console.ReadLine();
                char [] vetor = new char[cafe.Length];
                int cont = 0;
                for (int j = 0; j < dieta.Length; j++) {
                    for (int k = 0; k < cafe.Length; k++) {
                        if (dieta[j] == cafe[k])
                        {
                            vetor[cont] = cafe[k];
                            cont++;
                            cafe[k] = null;
                        }
                    }
                }
            }
        }
    }
}
