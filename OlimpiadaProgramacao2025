using System;

class Program
{
    static void Main(string[] args)
    {//Treino 01/04/2025
        ///////////////////////////BEECROWD//////////////////////////////////////
        string[] input = Console.ReadLine().Split();
        int X = int.Parse(input[0]);
        int Y = int.Parse(input[1]);
        int[,] matriz = new int[X, Y];
        for (int i = 0; i < X; i++)
        {
            string[] input2 = Console.ReadLine().Split();
            for (int j = 0; j < Y; j++)
            {
                matriz[i, j] = int.Parse(input2[j]);
            }
        }

        int cordx = 0, cordy = 0;
        for (int k = 1; k < X - 1; k++)
        {
            for (int l = 1; l < Y - 1; l++)
            {
                if (matriz[k, l] == 42)
                {
                    if (matriz[k, l - 1] == 7 && matriz[k, l + 1] == 7 && matriz[k - 1, l] == 7 && matriz[k + 1, l] == 7 && matriz[k - 1, l - 1] == 7 && matriz[k - 1, l + 1] == 7 && matriz[k + 1, l - 1] == 7 && matriz[k + 1, l + 1] == 7)
                    {
                        cordx = k + 1;
                        cordy = l + 1;
                    }
                }
            }
        }
        Console.WriteLine(cordx + " " + cordy);
