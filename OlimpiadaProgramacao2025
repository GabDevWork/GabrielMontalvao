/*
namespace OlimpiadaProgramacao2025
{
    internal class Program
    {
        static void Main(string[] args)
        {//SEGUNDA FASE
         ///////////////////////////BEECROWD//////////////////////////////////////
            ///cod 1958 Notação Científica
            string input = Console.ReadLine();
            string output = "";
            int ponto = 0;

            for(int k  = 0; k < input.Length; k++)
            {
                if (input[k] == '.')
                {
                    ponto = k;
                }
            }

            int i = 1;
            do
            {
                output += input[i];
                i++;
            } while (i < ponto);
            
            if(double.Parse(output) != 0.0)
            {
                output = "";
                if (input[0] == '-')
                {
                    output += "-" + input[1] + ".";
                    for (int j = 2; j < 5; j++)
                    {
                        Console.Write(input[j]);
                    }
                    if (ponto > 9) { Console.WriteLine("E+" + ponto); }
                    else
                        Console.WriteLine("E+0" + ponto);
                }
            }

        }
    }
}
