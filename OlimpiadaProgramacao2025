/*
///////////////////////////BEECROWD//////////////////////////////////////
///cod
string[] input = Console.ReadLine().Split();
int C = int.Parse(input[0]);
int N = int.Parse(input[1]);
string cifra = Console.ReadLine();
string cifrado = Console.ReadLine();
for (int i = 0; i < N; i++)
{
    string teste = "";
    teste = Console.ReadLine();

    for (int j = 0; j < teste.Length; j++)
    {
        bool cod = false;
        bool cif = false;
        bool eMaiuscula = false;
        int ind = 0;
        eMaiuscula = Char.IsUpper(teste[j]);
        for (int k = 0; k < C; k++)
        {
            if (teste[j].ToString().ToLower() == cifrado[k].ToString().ToLower())
            {
                cod = true;
                ind = k;
            }

            if (teste[j].ToString().ToLower() == cifra[k].ToString().ToLower())
            {
                cif = true;
                ind = k;
            }
        }
        if (!cod && !cif)
        {
            if (eMaiuscula)
                Console.Write(teste[j].ToString().ToUpper());
            else
                Console.Write(teste[j].ToString().ToLower());
        }
        else if(cod) {
            if (eMaiuscula)
                Console.Write(cifra[ind].ToString().ToUpper());
            else
                Console.Write(cifra[ind].ToString().ToLower());
        }
        else
        {
            if (eMaiuscula)
                Console.Write(cifrado[ind].ToString().ToUpper());
            else
                Console.Write(cifrado[ind].ToString().ToLower());
        }
    }
    if(i < N - 1)
    Console.WriteLine();
}
